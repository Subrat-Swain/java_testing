package com.anvsystems.oopdemo3;

public class Phone {
	String make;
	String model;
	String network;
	String screensize;
	
	Phone(String make, String model){
		this.make = make;
		this.model= model;
	}
	
	public static void main(String[] args) {
		Phone nokia = new Phone();// Default Constructor;
		
	}
}
