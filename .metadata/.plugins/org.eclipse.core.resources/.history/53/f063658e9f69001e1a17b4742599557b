package com.anvsystems.oopdemo3;

public class Server {
	
	public String serviceProvider;
	protected String ram;
	private String processor;
	
	public Server(String serviceProvider, String ram, String processor){
		this.serviceProvider = serviceProvider;
		this.ram = ram;
		this.processor = processor;
	}
//	public static void main(String[] args) {
//		Server s = new Server();
//		s.processor = "abcd";
//	}
	
	public void displayServerInfo() {
		System.out.println("############### SERVER INFO ###############");
		System.err.println(this.processor);
		System.err.println(this.ram);
		System.err.println(this.serviceProvider);
	}
}

